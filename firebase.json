{
    "hosting": {
        "public": "public",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "cleanUrls": true,
        "trailingSlash": false,
        "headers": [ {
            "source": "**/*",
            "headers": [ {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            } ]
          } ]
    }
}
